	<artifactId>projeto-site</artifactId>
	<version>1.0</version>
	<name>projeto-site</name>
	<packaging>pom</packaging>
	
	<!-- descricao do projeto -->
	<description>Descrição do projeto</description>
  	<url>www.meuprojeto.com.br</url>
  	<issueManagement>
