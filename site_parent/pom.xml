        		<sendOnFailure>true</sendOnFailure>
        		<sendOnWarning>false</sendOnWarning>
        		<address>test@address.com</address>
     		</notifier>
    	</notifiers>
  	</ciManagement>
  	<!-- desenvolvedores do projeto -->
  	<developers>
    	<developer>
      		<id>admin</id>
      		<name>Desenvolvedor 1</name>
